{
	"name": "OS2faktor Login Windows SSO",
	"version": "0.1.0",
	"minimum_chrome_version": "33",
	"description": "OS2faktor Login. Windows single sign-on",
	"permissions": ["background", "tabs", "storage"],
	"background": {
		"scripts": [ "libs/jquery-3.3.1/jquery.min.js", "background.js" ]
	},
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"icons": {
		"128": "images/icon_128.png"
	},
	"storage": {
		"managed_schema": "schema.json"
	},
	"web_accessible_resources": [
		"images/icon_128.png"
	],
	"manifest_version": 2
}